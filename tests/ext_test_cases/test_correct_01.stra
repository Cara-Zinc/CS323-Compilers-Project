struct Address {
    int street;
    char city;
    void print_addr(int street, char city);
    void display() {
        print_addr(street, city);
    }
};

struct Person {
    char nickname;
    int age;
    struct Address address;
    void print(char name[50], int age);
};

void main() {
    struct Person p;
    p.nickname = 'A';
    p.age = 30;
    struct Address addr;
    addr.street = 12;
    addr.city = 'w';
    p.address = addr;
    p.address.print_addr(addr.street, addr.city);
}
