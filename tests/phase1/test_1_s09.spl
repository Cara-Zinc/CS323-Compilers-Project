int reverse(int x) {
    #def 
    int rev = 0;
    #enddef 
    while x != 0) {
        #def 
        int 1_digit = x / 10;
        #enddef 
        if rev < 0 / 10 || rev > 1000000 / 10) {
            return 0;
        }
        x = x / 10;
        rev = rev * 10 + 1_digit;
    }
    return rev;
}
